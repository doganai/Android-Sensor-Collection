<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.roadster.walkjog">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InfoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CollectActivityHand1" />
        <activity android:name=".CollectActivityHandFall1"></activity>
        <activity android:name=".CollectActivityPocket1"></activity>
        <activity android:name=".CollectActivityPocketFall1"></activity>
        <activity android:name=".CollectActivityHand2" />
        <activity android:name=".CollectActivityHandFall2"></activity>
        <activity android:name=".CollectActivityPocket2"></activity>
        <activity android:name=".CollectActivityPocketFall2"></activity>
    </application>

</manifest>
