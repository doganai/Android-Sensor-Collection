    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="stair"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.roadster.stair.InfoActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.roadster.stair.CollectActivityHand1" />
        <activity android:name="com.example.roadster.stair.CollectActivityHandFall1" />
        <activity android:name="com.example.roadster.stair.CollectActivityPocket1" />
        <activity android:name="com.example.roadster.stair.CollectActivityPocketFall1" />
        <activity android:name="com.example.roadster.stair.CollectActivityHand2" />
        <activity android:name="com.example.roadster.stair.CollectActivityHandFall2" />
        <activity android:name="com.example.roadster.stair.CollectActivityPocket2" />
        <activity android:name="com.example.roadster.stair.CollectActivityPocketFall2" />
    </application>

</manifest>